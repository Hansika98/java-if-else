import java.util.Scanner;
public class text8 {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        System.out.println("enter the number = ");
        int a = sc.nextInt();
        if(a<0){
            int b = (-1)*a;
            System.out.println(b);
        }
        else
        System.out.println(a);
    }
}
